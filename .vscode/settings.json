{
  "editor.formatOnSave": true,
  "scss.validate": false,
  "[scss]": {
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
      "source.fixAll.stylelint": true
    }
  },
  "stylelint.configFile": "./.stylelintrc.js",
  "stylelint.validate": ["scss"],
  "cSpell.words": ["stylelint"]
}
